# editorconfig.org
# from https://github.com/google/web-starter-kit/blob/master/.editorconfig
# Changed eol to lf because of intra-mart recommendation.
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
